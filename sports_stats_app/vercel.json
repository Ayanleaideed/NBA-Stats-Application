{
  "version": 2,
  "builds": [
    {
      "src": "sports_stats_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sports_stats_app/wsgi.py"
    }
  ],
  "rewrites": {
    "sources": "/(.*)",
    "destination": "/"
  }
}